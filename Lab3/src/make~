g++ -O0 -g3 -Wall -c -fmessage-length=0 -MMD -MP -MF"Mierzalny.d" -MT"Mierzalny.o" -o "Mierzalny.o" "Mierzalny.cpp"

g++ -O0 -g3 -Wall -c -fmessage-length=0 -MMD -MP -MF"Stoper.d" -MT"Stoper.o" -o "Stoper.o" "Stoper.cpp"

g++ -O0 -g3 -Wall -c -fmessage-length=0 -MMD -MP -MF"TablicaO1.d" -MT"TablicaO1.o" -o "TablicaO1.o" "TablicaO1.cpp"

g++ -O0 -g3 -Wall -c -fmessage-length=0 -MMD -MP -MF"TablicaX2.d" -MT"TablicaX2.o" -o "TablicaX2.o" "TablicaX2.cpp"

g++ -O0 -g3 -Wall -c -fmessage-length=0 -MMD -MP -MF"main.d" -MT"main.o" -o "main.o" "main.cpp"

g++  -o "Program"  Mierzalny.o Stoper.o TablicaO1.o TablicaX2.o main.o   

